	"Address": {
		"prefix": "govuk-address",
		"body": [
			"{% call govukFieldset({",
			"\tlegend: {",
			"\t\ttext: \"${1:What is your address?}\",",
      "\t\tisPageHeading: ${2:true},",
			"\t\tclasses: \"govuk-fieldset__legend--${3:xl}\"",
			"\t}",
			"}) %}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-line-1\",",
			"\t\tname: \"address-line-1\",",
			"\t\tlabel: {",
			"\t\t\thtml: 'Building and street <span class=\"govuk-visually-hidden\">line 1 of 2</span>'",
			"\t\t},",
      "\t\tvalue: \"${5:Value}\",",
      "\t\tautocomplete: \"address-line1\"",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-line-2\",",
			"\t\tname: \"address-line-2\",",
			"\t\tlabel: {",
			"\t\t\thtml: '<span class=\"govuk-visually-hidden\">Building and street line 2 of 2</span>'",
			"\t\t},",
      "\t\tvalue: \"${6:Value}\",",
      "\t\tautocomplete: \"address-line2\"",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-town\",",
			"\t\tname: \"address-town\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Town or city\"",
			"\t\t},",
      "\t\tvalue: \"${7:Value}\",",
      "\t\tautocomplete: \"address-level2\",",
			"\t\tclasses: \"govuk-!-width-two-thirds\"",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-county\",",
			"\t\tname: \"address-county\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"County\"",
			"\t\t},",
      "\t\tvalue: \"${8:Value}\",",
			"\t\tclasses: \"govuk-!-width-two-thirds\"",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-postcode\",",
			"\t\tname: \"address-postcode\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Postcode\"",
			"\t\t},",
      "\t\tvalue: \"${9:Value}\",",
      "\t\tautocomplete: \"postal-code\",",
			"\t\tclasses: \"govuk-input--width-10\"",
			"\t}) }}",
			"",
			"{% endcall %}$0"
		],
		"description": "Addresses – GOV.UK Design System"
	},
	"Date input": {
		"prefix": "govuk-date",
		"body": [
			"{{ govukDateInput({",
			"\tid: \"${1:date}\",",
			"\tname: \"${1:date}\",",
			"\tfieldset: {",
			"\t\tlegend: {",
			"\t\t\ttext: \"${2:Legend text}\",",
			"\t\t\tisPageHeading: ${3:true},",
			"\t\t\tclasses: \"govuk-fieldset__legend--${4:xl}\"",
			"\t\t}",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${5:For example, 31 3 1980}\"",
			"\t},",
			"\titems: [",
			"\t\t{",
			"\t\t\tname: \"day\",",
      "\t\t\tvalue: \"${6:Value}\"",
			"\t\t},",
			"\t\t{",
			"\t\t\tname: \"month\",",
      "\t\t\tvalue: \"${7:Value}\"",
			"\t\t},",
			"\t\t{",
			"\t\t\tname: \"year\",",
      "\t\t\tvalue: \"${8:Value}\"",
			"\t\t}",
			"\t]",
			"}) }}$0"
		],
		"description": "Date input – GOV.UK Design System"
	},
	"File upload": {
		"prefix": "govuk-file",
		"body": [
			"{{ govukFileUpload({",
			"\tid: \"${1:file-upload-1}\",",
			"\tname: \"${1:file-upload-1}\",",
			"\tlabel: {",
			"\t\ttext: \"${2:Upload a file}\"",
			"\t}",
			"\thint: {",
			"\t\ttext: \"${3:Hint text}\"",
			"\t}",
			"}) }}$0"
		],
		"description": "File upload – GOV.UK Design System"
	},
	"National Insurance number (NINO)": {
		"prefix": "govuk-nino",
		"body": [
			"{{ govukInput({",
			"\tid: \"national-insurance-number\",",
			"\tname: \"national-insurance-number\",",
			"\tlabel: {",
			"\t\ttext: \"National insurance number\"",
			"\t},",
			"\thint: {",
			"\t\ttext: \"It’s on your National Insurance card, benefit letter, payslip or P60. For example, ‘QQ 12 34 56 C’.\"",
			"\t},",
      "\tvalue: \"${1:Value}\"",
			"\tclasses: \"govuk-input--width-10\"",
			"}) }}$0"
		],
		"description": "National Insurance number (NINO) – GOV.UK Design System"
