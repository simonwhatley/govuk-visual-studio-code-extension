			"\t\t}",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-town\",",
			"\t\tname: \"address-town\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Town or city\"",
			"\t\t},",
			"\t\tclasses: \"govuk-!-width-two-thirds\"",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-county\",",
			"\t\tname: \"address-county\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"County\"",
			"\t\t},",
			"\t\tclasses: \"govuk-!-width-two-thirds\"",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-postcode\",",
			"\t\tname: \"address-postcode\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Postcode\"",
			"\t\t},",
			"\t\tclasses: \"govuk-input--width-10\"",
			"\t}) }}",
			"",
			"{% endcall %}$4",
			"$0"
		],
		"description": "Addresses – GOV.UK Design System"
	},
