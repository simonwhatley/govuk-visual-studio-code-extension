		"prefix": "govuk-section-break",
		"body": [
		  "{# Size modifiers: xl, l, m #}",
		  "<hr class=\"govuk-section-break govuk-section-break--${1:size} govuk-section-break--visible\">$0"
		],
		"description": "Section break – GOV.UK Design System"
	},
	"Headings": {
		"prefix": "govuk-heading",
		"body": [
			"<h${1:1} class=\"govuk-heading-${2:xl|l|m}\">",
			"\t$3",
			"</h${1:1}>$0"
		],
		"description": "Headings – GOV.UK Design System"
	},
	"Captions": {
		"prefix": "govuk-caption",
