		"body": [
			"{",
			"\ttext: \"$1\",",
			"\thref: \"$2\"",
			"}$0"
		],
		"description": "Link item – GOV.UK Design System"
	},
	"Panel": {
		"prefix": "govuk-panel",
		"body": [
			"{{ govukPanel({",
			"\ttitleText: \"${1:Title text}\",",
			"\thtml: \"Your reference number is<br><strong>$2</strong>\"",
			"}) }}$0"
		],
		  "description": "Panel – GOV.UK Design System"
