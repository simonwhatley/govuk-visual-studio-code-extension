		],
		"description": "Skip link – GOV.UK Design System"
	},
	"Table": {
		"prefix": "govuk-table",
		"body": [
			"{{ govukTable({",
			"\tcaption: \"${1:Caption text}\",",
			"\tfirstCellIsHeader: ${2:false},",
			"\thead: [",
			"\t\t{",
			"\t\t\ttext: \"Column 1 heading\"",
