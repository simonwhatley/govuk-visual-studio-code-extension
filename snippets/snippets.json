	},
	"Button": {
		"prefix": "govuk-button",
		"body": [
			"{{ govukButton({",
			"\ttext: \"${1:Button text}\"",
			"}) }}$0"
		],
		"description": "Button – GOV.UK Design System"
	  },
	"Checkboxes": {
		"prefix": "govuk-checkboxes",
		"body": [
			"{{ govukCheckboxes({",
			"\tidPrefix: \"$1\",",
			"\tname: \"$1\",",
			"\tfieldset: {",
			"\t\tlegend: {",
			"\t\t\ttext: \"${2:Legend text}\",",
			"\t\t\tisPageHeading: ${3:true},",
			"\t\t\tclasses: \"govuk-fieldset__legend--${4:xl}\"",
			"\t\t}",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${5:Hint text}\"",
			"\t},",
			"\titems: [",
      "\t\t{",
      "\t\t\tvalue: \"${6:Value}\",",
      "\t\t\ttext: \"${7:Text}\",",
      "\t\t\tchecked: $8",
			"\t\t}$9",
			"\t]",
			"}) }}$0"
		],
		"description": "Checkboxes – GOV.UK Design System"
	},
	"Radios": {
		"prefix": "govuk-radios",
		"body": [
			"{{ govukRadios({",
			"\tidPrefix: \"$1\",",
			"\tname: \"$1\",",
			"\tfieldset: {",
			"\t\tlegend: {",
			"\t\t\ttext: \"${2:Legend text}\",",
			"\t\t\tisPageHeading: ${3:true},",
			"\t\t\tclasses: \"govuk-fieldset__legend--${4:xl}\"",
			"\t\t}",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${5:Hint text}\"",
			"\t},",
			"\titems: [",
      "\t\t{",
      "\t\t\tvalue: \"${6:Value}\",",
      "\t\t\ttext: \"${7:Text}\",",
      "\t\t\tchecked: $8",
			"\t\t}$9",
			"\t]",
			"}) }}$0"
		],
		"description": "Radios – GOV.UK Design System"
	},
	"Select": {
		"prefix": "govuk-select",
		"body": [
			"{{ govukSelect({",
			"\tid: \"$1\",",
			"\tname: \"$1\",",
			"\tlabel: {",
			"\t\ttext: \"${2:Label text}\"",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${3:Hint text}\"",
			"\t},",
			"\titems: [",
      "\t\t{",
      "\t\t\tvalue: \"${4:Value}\",",
      "\t\t\ttext: \"${5:Text}\",",
      "\t\t\tselected: $6",
			"\t\t}$7",
			"\t]",
			"}) }}$0"
		],
		"description": "Select – GOV.UK Design System"
	},
	"Radio or checkbox option": {
		"prefix": "govuk-option",
		"body": [
			"{",
      "\tvalue: \"$1\",",
      "\ttext: \"$2\",",
