		"prefix": "govuk-list",
		"body": [
			"<ul class=\"govuk-list\">",
			"\t<li>",
			"\t\t${1:List item}",
			"\t</li>",
			"\t<li>",
			"\t\t${2:List item}",
			"\t</li>",
			"\t<li>",
			"\t\t${3:List item}",
			"\t</li>",
			"</ul>$0"
		],
		"description": "Lists – GOV.UK Design System"
	},
	"Bulleted lists": {
		"prefix": "govuk-list-bullet",
		"body": [
			"<ul class=\"govuk-list govuk-list--bullet\">",
			"\t<li>",
			"\t\t${1:List item}",
			"\t</li>",
			"\t<li>",
			"\t\t${2:List item}",
			"\t</li>",
			"\t<li>",
			"\t\t${3:List item}",
