	"Address": {
		"prefix": "govuk-form-address",
		"body": [
			"{% call govukFieldset({",
			"\tlegend: {",
			"\t\ttext: \"${1:What is your address?}\",",
			"\t\tclasses: \"govuk-fieldset__legend--${2:xl}\",",
			"\t\tisPageHeading: ${3:true}",
			"\t}",
			"}) %}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-line-1\",",
			"\t\tname: \"address-line-1\",",
			"\t\tlabel: {",
			"\t\t\thtml: 'Building and street <span class=\"govuk-visually-hidden\">line 1 of 2</span>'",
			"\t\t}",
			"\t}) }}",
			"",
			"\t{{ govukInput({",
			"\t\tid: \"address-line-2\",",
